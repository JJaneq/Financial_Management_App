<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="220.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.edp.projekt.controller.UserEditController">
   <children>
      <Label fx:id="profileLabel" alignment="CENTER" layoutX="26.0" layoutY="14.0" text="Profil: " AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" layoutX="-143.0" layoutY="51.0" promptText="Nazwa profilu" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="60.0" />
      <TextField fx:id="moneyTextField" layoutX="20.0" layoutY="61.0" promptText="Dostępne środki" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="105.0" />
      <TextField fx:id="monthlyLimitTextField" layoutX="30.0" layoutY="71.0" promptText="Miesięczny limit" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="150.0" />
      <Button fx:id="saveButton" mnemonicParsing="false" onAction="#onSaveButtonClicked" text="Zapisz" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="50.0" />
      <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onCancelButtonClicked" text="Anuluj" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="50.0" />
      <Label text="Nazwa profilu" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="40.0" />
      <Label layoutX="20.0" layoutY="50.0" text="Dostępne środki" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="85.0" />
      <Label layoutX="20.0" layoutY="95.0" text="Miesięczny limit" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="130.0" />
   </children>
</AnchorPane>
